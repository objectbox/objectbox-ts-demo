//namespace objectbox.tsdemo;

/// Time series object
table SensorValues {
    id: ulong;

    /// objectbox:id-companion,date
    time: long;

    temperatureOutside: double;
    temperatureInside: double;

    temperatureCpu: double;
    loadCpu1: double;
    loadCpu2: double;
    loadCpu3: double;
    loadCpu4: double;
}

table NamedTimeRange {
    id: ulong;

    begin: long;
    end: long;

    name: string;
}

